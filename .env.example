# Client-side environment variables (exposed to browser)
# These are prefixed with VITE_ and will be bundled into the client JavaScript
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_AMAZON_AFFILIATE_TAG=aiautorepai04-20

# Server-side only environment variables (NOT exposed to browser)
# These are only accessible in server-side code (api/ folder, serverless functions)
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
GEMINI_API_KEY=your_gemini_api_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret